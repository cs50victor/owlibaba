{
  "name": "nextwin",
  "description": "Easy Next.js & Twin.macro Web dev Template .",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Victor A.",
    "url": "https://victoruniverse.com"
  },
  "scripts": {
    "dev": "yarn format && yarn lint && next dev -p 8000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "yarn prettier --write \"**/*.+(js|jsx|json|md|ts|tsx)\""
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@hookform/resolvers": "^2.6.0",
    "@styled-icons/simple-icons": "^10.33.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.2.1",
    "next": "latest",
    "next-themes": "^0.0.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.9.0",
    "react-toggle-dark-mode": "^1.0.4",
    "styled-components": "^5.3.0",
    "tailwindcss": "^2.2.2",
    "twin.macro": "^2.5.0",
    "use-sound": "^4.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.1",
    "react-is": "^17.0.2"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "includeClassNames": false
    }
  },
  "prettier": {
    "arrowParens": "always",
    "doubleQuote": true,
    "tabWidth": 2,
    "printWidth": 90,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "semi": false
  }
}
